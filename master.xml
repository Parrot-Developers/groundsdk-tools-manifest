 <project name="json-c" path="packages/json" revision="json-0.15-branch"/>
 <project name="libfutils" path="packages/libfutils"/>
 <project name="libh264" path="packages/libh264"/>
 <project name="libh265" path="packages/libh265"/>
 <project name="libmedia-buffers" path="packages/libmedia-buffers"/>
 <project name="libmetadata-thermal" path="packages/libmetadata-thermal"/>
